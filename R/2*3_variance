a <- 1/6
b <- 1/6
c <- 1/6
d <- 1/6
e <- 1/6
f <- 1/6
V1 <- a*(1-a)
V2 <- b*(1-b)
V3 <- c*(1-c)
V4 <- d*(1-d)
V5 <- d*(1-d)
V6 <- e*(1-e)
V12 <- a*b
V13 <- a*c
V14 <- a*d
V15 <- a*e
V16 <- a*f
V23 <- b*c
V24 <- b*d
V25 <- b*e
V26 <- b*f
V34 <- c*d
V35 <- c*e
V36 <- c*f
V45 <- d*e
V46 <- d*f
V56 <- e*f
D1 <- a/(a+b+c) - (1/(a+b+c)^2)*(a^2/(a+d) + b^2/(b+e) + c^2/(c+f)) + 
(d/(d+e+f)^2)*(a/(a+b+c) - d/(d+e+f))
D1
D2 <- b/(a+b+c) - (1/(a+b+c)^2)*(a^2/(a+d) + b^2/(b+e) + c^2/(c+f)) + 
(e/(d+e+f)^2)*(b/(a+b+c) - e/(d+e+f))
D2
D3 <- c/(a+b+c) - (1/(a+b+c)^2)*(a^2/(a+d) + b^2/(b+e) + c^2/(c+f)) + 
(f/(d+e+f)^2)*(c/(a+b+c) - f/(d+e+f))
D3
D4 <- d/(d+e+f) - (1/(d+e+f)^2)*(d^2/(a+d) + e^2/(b+e) + f^2/(c+f)) + 
(a/(a+b+c)^2)*(d/(d+e+f) - a/(a+b+c))
D4
D5 <- e/(d+e+f) - (1/(d+e+f)^2)*(d^2/(a+d) + e^2/(b+e) + f^2/(c+f)) + 
(b/(a+b+c)^2)*(e/(d+e+f) - b/(a+b+c))
D5
D6 <- f/(d+e+f) - (1/(d+e+f)^2)*(d^2/(a+d) + e^2/(b+e) + f^2/(c+f)) + 
(c/(a+b+c)^2)*(f/(d+e+f) - c/(a+b+c))
D5
SV2 <- (a^2)/((a+b+c)*(a+d)) + (b^2)/((a+b+c)*(b+e)) + (c^2)/((a+b+c)*(c+f)) +
(d^2)/((d+e+f)*(a+d)) + (e^2)/((d+e+f)*(b+e)) + (f^2)/((d+e+f)*(c+f)) - 1
SV2
VAR <- V1*D1^2 + V2*D2^2 + V3*D3^2 + V4*D4^2 + V5*D5^2 + V6*D6^2 + 2*V12*D1*D2 +
2*V13*D1*D3 + 2*V14*D1*D4 + 2*V15*D1*D5 + 2*V16*D1*D6 + 2*V23*D2*D3 + 
2*V24*D2*D4 + 2*V25*D2*D5 + 2*V26*D2*D6 + 2*V34*D3*D4 + 2*V35*D3*D5 + 
2*V36*D3*D6 + 2*V45*D4*D5 + 2*V46*D4*D6 + 2*V56*D5*D6
VAR 
